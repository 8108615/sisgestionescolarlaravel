Table configuraciones {
    id int [pk, increment] //primary key
    nombre varchar
    descripcion varchar
    direccion varchar
    telefono varchar
    divisa varchar
    correo_electronico varchar
    web varchar
    logo varchar
}

Table gestiones {
    id int [pk, increment] //primary key
    nombre varchar
}
Table periodos {
    id int [pk, increment] //primary key
    nombre varchar
    gestion_id int [ref: > gestiones.id]
}
Table niveles {
    id int [pk, increment] //primary key
    nombre varchar
}
Table grados {
    id int [pk, increment] //primary key
    nombre varchar
    nivel_id int [ref: > niveles.id]
}
Table paralelos {
    id int [pk, increment] //primary key
    nombre varchar
    grado_id int [ref: > grados.id]
}
Table turnos {
    id int [pk, increment] //primary key
    nombre varchar
}
Table materias {
    id int [pk, increment] //primary key
    nombre varchar
}

Table users {
    id int [pk, increment] //primary key
    name varchar
    email varchar
    password varchar
}

Table personals {
    id int [pk, increment] //primary key
    usuario_id int [ref: > users.id]
    tipo varchar
    nombres varchar
    apellidos varchar
    ci varchar
    fecha_nacimiento date
    direccion varchar
    telefono varchar
    profesion varchar
    foto varchar
}